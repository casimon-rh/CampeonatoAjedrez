@model IEnumerable<CampeonatoAjedrezAPI.Areas.CAMPEONATO_ANTERIOR>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FECHA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NOMBRE)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FECHA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NOMBRE)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDCAMPEONATO }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDCAMPEONATO }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDCAMPEONATO })
        </td>
    </tr>
}

</table>
